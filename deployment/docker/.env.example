# PostgreSQL configuration
POSTGRES_USER=space_food
POSTGRES_PASSWORD=changeme-in-production
POSTGRES_DB=space_food

# Backend configuration
JWT_SECRET=change-this-secret-in-production-use-long-random-string
LOG_LEVEL=info

# AI Configuration (optional)
# Ollama
SPACE_FOOD_AI_OLLAMA_ENABLED=true
SPACE_FOOD_AI_OLLAMA_HOST=http://localhost:11434
SPACE_FOOD_AI_OLLAMA_MODEL=llama2

# OpenAI (optional)
SPACE_FOOD_AI_OPENAI_ENABLED=false
SPACE_FOOD_AI_OPENAI_APIKEY=your-openai-api-key
SPACE_FOOD_AI_OPENAI_MODEL=gpt-3.5-turbo

# Gemini (optional)
SPACE_FOOD_AI_GEMINI_ENABLED=false
SPACE_FOOD_AI_GEMINI_APIKEY=your-gemini-api-key
SPACE_FOOD_AI_GEMINI_MODEL=gemini-pro

# Claude (optional)
SPACE_FOOD_AI_CLAUDE_ENABLED=false
SPACE_FOOD_AI_CLAUDE_APIKEY=your-claude-api-key
SPACE_FOOD_AI_CLAUDE_MODEL=claude-3-sonnet-20240229
